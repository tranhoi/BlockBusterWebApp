<!DOCTYPE html>
<html lang="en">
<head>
    <title>BeanShop - Smartphone and tablet</title>
    <link rel="icon" type="image/png" href="~/images/Smartphone.ico" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700" rel="stylesheet">
    <!-- Exzoom -->
    <link rel="stylesheet" type="text/css" href="~/Content/jquery.exzoom.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Site.css" />
    <!-- Bootstrap -->
    <link type="text/css" rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <!-- Slick -->
    <link type="text/css" rel="stylesheet" href="~/Content/slick.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/slick-theme.css" />
    <!-- nouislider -->
    <link type="text/css" rel="stylesheet" href="~/Content/nouislider.min.css" />
    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/Content/style.css" />
    <!-- My stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/Content_me/Mystyle.css" />
    <!-- Contract page stlylesheet -->
    <link href="~/Content_me/main.contract.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>
<body>
    <!-- HEADER -->
    <header>
        <!-- top Header -->
        <div id="top-header">
            <div class="container">
                <div class="pull-left">
                    <span>Welcome to Bean Store!</span>
                </div>
                <div class="pull-right">
                    <ul class="header-top-links">
                        <li class="dropdown default-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">ENG <i class="fa fa-caret-down"></i></a>
                            <ul class="custom-menu">
                                <li><a href="#">English (ENG)</a></li>
                                <li><a href="#">Vietnamese (VIE)</a></li>
                            </ul>
                        </li>
                        <li class="dropdown default-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">USD <i class="fa fa-caret-down"></i></a>
                            <ul class="custom-menu">
                                <li><a href="#">USD ($)</a></li>
                                <li><a href="#">VND (Đ)</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /top Header -->
        <!-- header -->
        <div id="header">
            <div class="container">
                <div class="pull-left">
                    <!-- Logo -->
                    <div class="header-logo">
                        <a href="@Url.Action("Index", "Bean")">
                            <img src="~/images/logo.png" alt="logo" width="200" , height="80">
                        </a>
                    </div>
                    <!-- /Logo -->
                    <!-- Search -->
                    <div class="header-search" style="width: 60%">
                        @Html.Action("Search", "Bean")
                    </div>
                    <!-- /Search -->
                </div>
                <div class="pull-right">
                    <ul class="header-btns">
                        <!-- Account -->
                        @Html.Action("Account", "User")
                        <!-- /Account -->
                        <!-- Cart -->
                        @Html.Action("CartPartial", "Cart")
                        <!-- /Cart -->
                        <!-- Mobile nav toggle-->
                        <li class="nav-toggle">
                            <button class="nav-toggle-btn main-btn icon-btn"><i class="fa fa-bars"></i></button>
                        </li>
                        <!-- / Mobile nav toggle -->
                    </ul>
                </div>
            </div>
            <!-- header -->
        </div>
        <!-- container -->
    </header>
    <!-- /HEADER -->
    <!-- NAVIGATION -->
    <div id="navigation">
        <!-- container -->
        <div class="container">
            <div id="responsive-nav">
                <!-- category nav -->
                <div class="category-nav show-on-click">
                    <span class="category-header">Choose your brand<i class="fa fa-list"></i></span>
                    <ul class="category-list">
                        <li class="dropdown side-dropdown">
                            @Html.Action("Brand", "Bean")
                        </li>
                        <li><a href="@Url.Action("Item_All", "Bean")">View All</a></li>
                    </ul>
                </div>
                <!-- /category nav -->
                <!-- menu nav -->
                <div class="menu-nav">
                    <span class="menu-header">Menu <i class="fa fa-bars"></i></span>
                    <ul class="menu-list">
                        <li><a href="@Url.Action("Index", "Bean")">Home</a></li>
                        <li class="dropdown side-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Smart phone <i class="fa fa-angle-right"></i></a>
                            <div class="custom-menu">
                                <div class="row">
                                    <div class="col-md-4">
                                        <ul class="list-links">
                                            <li>
                                                <h3 class="list-links-title" style="color:#F8694A">Brand</h3>
                                            </li>
                                            @Html.Action("Catalog_Brand", "Bean", new { id = 1 })
                                        </ul>
                                        <hr class="hidden-md hidden-lg">
                                    </div>
                                    <div class="col-md-4">
                                        <ul class="list-links">
                                            <li>
                                                <h3 class="list-links-title" style="color:#F8694A">Ranked</h3>
                                            </li>
                                            @Html.Action("Catalog_Ranked", "Bean", new { id = 1 })
                                        </ul>
                                        <hr class="hidden-md hidden-lg">
                                    </div>
                                </div>
                                <div class="row hidden-sm hidden-xs">
                                    <div class="col-md-12">
                                        <hr>
                                        <a class="banner banner-1" href="@Url.Action("Items_Catalog", "Bean", new { id = 1})">
                                            <img src="~/images_banner/banner01.png" />
                                            <div class="banner-caption text-center">
                                                <h3 class="white-color text-uppercase">view all smartphone</h3>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li class="dropdown side-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Tablet <i class="fa fa-angle-right"></i></a>
                            <div class="custom-menu" style="width:500px">
                                <div class="row">
                                    <div class="col-md-4" style="width:50%">
                                        <ul class="list-links">
                                            <li>
                                                <h3 class="list-links-title">Brand</h3>
                                            </li>
                                            @Html.Action("Catalog_Brand", "Bean", new { id = 2 })
                                        </ul>
                                        <hr>
                                        <ul class="list-links">
                                            <li>
                                                <h3 class="list-links-title">Ranked</h3>
                                            </li>
                                            @Html.Action("Catalog_Ranked", "Bean", new { id = 2 })
                                        </ul>
                                        <hr class="hidden-md hidden-lg">
                                    </div>
                                    <div class="col-md-4" style="width:50%">
                                        <a class="banner banner-2" href="@Url.Action("Items_Catalog", "Bean", new { id = 2})">
                                            <img src="~/images/mensfashion-priv-closeup.jpg" />
                                            <div class="banner-caption text-center">
                                                <h3 class="white-color">VIEW ALL<br>Tablet</h3>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li><a href="@Url.Action("Items_Catalog", "Bean", new { id = 3})">Gear</a></li>
                        <li><a href="@Url.Action("About", "Bean" )">About</a></li>
                        <li><a href="@Url.Action("Contact", "Bean")">Contact</a></li>
                    </ul>
                </div>
                <!-- menu nav -->
            </div>
        </div>
        <!-- /container -->
    </div>
    <!-- /NAVIGATION -->
    <!-- render body -->
    <div>
        @RenderBody()
    </div>
    <!-- Your customer chat code -->
    <div class="fb-customerchat"
         attribution=setup_tool
         page_id="488909728518807"
         theme_color="#F8694A">
    </div>
    <!-- /render body -->
    <!-- FOOTER -->
    <footer id="footer" class="section section-grey">
        <!-- container -->
        <div class="container">
            <hr>
            <!-- fanpage-->
            <div class="fb-page" data-href="https://www.facebook.com/Bean-Shop-488909728518807/" data-tabs="timeline" data-width="300" data-height="300" data-small-header="true" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true"><blockquote cite="https://www.facebook.com/Bean-Shop-488909728518807/" class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/Bean-Shop-488909728518807/">Bean Shop</a></blockquote></div>
            <!-- / fanpage-->
            <!-- row -->
            <div class="col-md-8 col-sm-6 col-xs-6">
                <!-- footer logo -->
                <div class="footer-logo">
                    <div>
                        <img src="~/images/logo.png" alt="logo" width="300" , height="120">
                    </div>
                </div>
                <!-- /footer logo -->
                <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna</p>
                <!-- footer social -->
                <ul class="footer-social">
                    <li><a href="https://www.facebook.com/Bean-Shop-488909728518807"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="https://www.youtube.com/channel/UCBfwswJEpRVGE8jW-JXuWYQ?view_as=subscriber"><i class="fa fa-youtube-play"></i></a></li>
                    <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                    <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                    <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
                </ul>
                <!-- /footer social -->
            </div>
            <div class="col-md-8 col-md-offset-2 text-center">
                <!-- footer copyright -->
                <div class="footer-copyright">
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    <script>document.write(new Date().getFullYear());</script>
                    &copy;
                    <a href="@Url.Action("Login", "Admin")" target="_blank">Patheo</a> <i class="fa fa-heart-o" aria-hidden="true"></i> dz Vailone
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </div>
                <!-- /footer copyright -->
            </div>
            <!-- /row -->
        </div>
        <!-- /container -->
    </footer>
    <!-- /FOOTER -->
    <!-- jQuery Plugins -->
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v3.3"></script>
    <script src="https://code.jquery.com/jquery-3.4.0.js"></script>
    <script src="~/Scripts/jquery.exzoom.js"></script>
    <script type="text/javascript">
        $(function () {
            $("#exzoom").exzoom({
                // autoplay
                "autoPlay": false,
                // autoplay interval in milliseconds
                "autoPlayTimeout": 2000
            });
        });
    </script>
    <script>
        // Get the modal
        var modal = document.getElementById('messbox');
        // When the user clicks anywhere outside of the modal, close it
        window.onclick = function (event) {
            if (event.target == modal) {
                modal.style.display = "none";
            }
        }
    </script>
    <!--
    <script src="~/Scripts/jquery.min.js"></script>
    -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/slick.min.js"></script>
    <script src="~/Scripts/nouislider.min.js"></script>
    <script src="~/Scripts/jquery.zoom.min.js"></script>
    <script src="~/Scripts/main.js"></script>

    <!-- Load Facebook SDK for JavaScript -->
    <div id="fb-root"></div>
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                xfbml: true,
                version: 'v3.3'
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.customerchat.js';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>
    <style>
        .alert {
            padding: 20px;
            background-color: #F8694A;
            color: white;
        }

        .alert2 {
            padding: 20px;
            background-color: darkslategrey;
            color: white;
        }

        .closebtn {
            margin-left: 15px;
            color: white;
            font-weight: bold;
            float: right;
            font-size: 22px;
            line-height: 20px;
            cursor: pointer;
            transition: 0.3s;
        }

            .closebtn:hover {
                color: black;
            }
    </style>
</body>
</html>